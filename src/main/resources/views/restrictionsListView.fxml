<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="570.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="450.0" prefWidth="300.0">
         <children>
            <HBox prefHeight="30.0" prefWidth="300.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Add" />
                  <Button fx:id="editButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Edit" />
                  <Button fx:id="removeButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" text="Remove" />
               </children>
            </HBox>
            <ListView fx:id="restrictionListView" prefHeight="540.0" prefWidth="300.0" />
         </children>
      </VBox>
      <TabPane fx:id="restrictionTabPane" prefHeight="570.0" prefWidth="500.0" tabClosingPolicy="UNAVAILABLE" />
   </children>
</HBox>
